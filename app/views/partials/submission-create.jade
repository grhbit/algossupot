      .form-group
        label(for='') 언어
        select(name='').form-control
          option(value="cpp") C++
          option(value="c") C
      .form-group
        label(for='') 소스코드
        input.form-control(type='file' ng-model='file')

      button.btn.btn-primary(ng-click='submit()') Submit
